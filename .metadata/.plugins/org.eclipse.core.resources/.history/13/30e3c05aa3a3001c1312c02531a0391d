package com.userAndProduct.Apirest.model;

public class ListarDeptEQtdTarefas {
	
	public long idDepartamento;
	public String deptTitulo;
	public long idUser;
	
	public long getIdDepartamento() {
		return idDepartamento;
	}
	public void setIdDepartamento(long idDepartamento) {
		this.idDepartamento = idDepartamento;
	}
	public String getDeptTitulo() {
		return deptTitulo;
	}
	public void setDeptTitulo(String deptTitulo) {
		this.deptTitulo = deptTitulo;
	}
	public long getIdUser() {
		return idUser;
	}
	public void setIdUser(long idUser) {
		this.idUser = idUser;
	}
	
	public ListarDeptEQtdTarefas(long idDepartamento, String deptTitulo, long idUser) {
		super();
		this.idDepartamento = idDepartamento;
		this.deptTitulo = deptTitulo;
		this.idUser = idUser;
	}
	
}
